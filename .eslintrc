{
  "parser": "babel-eslint",
  "extends": [
      "airbnb",
      "prettier",
      "prettier/react"
  ],
  "plugins": ["prettier"],
  "env": {
      "browser": true,
      "node": true,
      "es6": true
  },
  "rules": {
      "prettier/prettier": "error",
      "react/prop-types": 0,
      "react/jsx-props-no-spreading": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}